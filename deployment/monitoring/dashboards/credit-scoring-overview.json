{
  "dashboard": {
    "id": null,
    "uid": "credit-scoring-overview",
    "title": "Credit Scoring - Overview",
    "tags": ["credit-scoring", "mlops", "production"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate & Error Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{namespace=\"credit-scoring-production\"}[5m]))",
            "legendFormat": "Requests/sec",
            "refId": "A"
          },
          {
            "expr": "sum(rate(http_requests_total{namespace=\"credit-scoring-production\", status!~\"2..\"}[5m]))",
            "legendFormat": "Errors/sec",
            "refId": "B"
          },
          {
            "expr": "http_error_rate{namespace=\"credit-scoring-production\"}",
            "legendFormat": "Error Rate %",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "reqps", "min": 0},
          {"format": "percent", "min": 0, "max": 100}
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [5], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["C", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "message": "Error rate above 5%",
          "name": "High Error Rate Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 2,
        "title": "Response Latency (Percentiles)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "http_request_duration_seconds:p50{namespace=\"credit-scoring-production\"}",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "http_request_duration_seconds:p95{namespace=\"credit-scoring-production\"}",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "http_request_duration_seconds:p99{namespace=\"credit-scoring-production\"}",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "min": 0}
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "thresholds": [
          {"colorMode": "critical", "fill": true, "line": true, "op": "gt", "value": 1}
        ]
      },
      {
        "id": 3,
        "title": "Model Predictions",
        "type": "stat",
        "gridPos": {"x": 0, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(model_predictions_total{namespace=\"credit-scoring-production\"}[5m]))",
            "legendFormat": "",
            "refId": "A"
          }
        ],
        "format": "ops",
        "valueName": "current",
        "thresholds": "10,50",
        "colors": ["#299c46", "rgba(237, 129, 40, 0.89)", "#d44a3a"],
        "sparkline": {"fillColor": "rgba(31, 118, 189, 0.18)", "show": true}
      },
      {
        "id": 4,
        "title": "Model Failure Rate",
        "type": "gauge",
        "gridPos": {"x": 6, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "model_failure_rate{namespace=\"credit-scoring-production\"}",
            "legendFormat": "",
            "refId": "A"
          }
        ],
        "format": "percent",
        "thresholds": "1,5",
        "max": 100,
        "min": 0,
        "valueName": "current"
      },
      {
        "id": 5,
        "title": "Resource Utilization",
        "type": "graph",
        "gridPos": {"x": 0, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "container_cpu_utilization{namespace=\"credit-scoring-production\", container=\"api\"}",
            "legendFormat": "CPU",
            "refId": "A"
          },
          {
            "expr": "container_memory_utilization{namespace=\"credit-scoring-production\", container=\"api\"}",
            "legendFormat": "Memory",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "percent", "min": 0, "max": 100}
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "thresholds": [
          {"colorMode": "warning", "fill": true, "line": true, "op": "gt", "value": 80},
          {"colorMode": "critical", "fill": true, "line": true, "op": "gt", "value": 90}
        ]
      },
      {
        "id": 6,
        "title": "Pod Status",
        "type": "table",
        "gridPos": {"x": 12, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "kube_pod_status_phase{namespace=\"credit-scoring-production\", pod=~\"credit-scoring-api-.*\"}",
            "legendFormat": "{{pod}}",
            "refId": "A"
          }
        ],
        "columns": [
          {"text": "Pod", "value": "pod"},
          {"text": "Status", "value": "value"},
          {"text": "Phase", "value": "phase"}
        ],
        "styles": [
          {
            "alias": "Pod",
            "pattern": "pod",
            "type": "string"
          },
          {
            "alias": "Status",
            "colorMode": "value",
            "colors": ["rgba(50, 172, 45, 0.97)", "rgba(237, 129, 40, 0.89)", "rgba(245, 54, 54, 0.9)"],
            "pattern": "value",
            "thresholds": ["0.5", "1"],
            "type": "number",
            "unit": "short"
          },
          {
            "alias": "Phase",
            "colorMode": "value",
            "colors": ["rgba(50, 172, 45, 0.97)", "rgba(237, 129, 40, 0.89)", "rgba(245, 54, 54, 0.9)"],
            "pattern": "phase",
            "thresholds": [],
            "type": "string"
          }
        ]
      },
      {
        "id": 7,
        "title": "Business Metrics",
        "type": "graph",
        "gridPos": {"x": 0, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "loan_approval_rate{namespace=\"credit-scoring-production\"}",
            "legendFormat": "Approval Rate",
            "refId": "A"
          },
          {
            "expr": "high_risk_rate{namespace=\"credit-scoring-production\"}",
            "legendFormat": "High Risk Rate",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "percent", "min": 0, "max": 100}
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2
      },
      {
        "id": 8,
        "title": "Data Drift Monitoring",
        "type": "graph",
        "gridPos": {"x": 12, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "data_drift_score",
            "legendFormat": "Data Drift",
            "refId": "A"
          },
          {
            "expr": "concept_drift_score",
            "legendFormat": "Concept Drift",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "percent", "min": 0, "max": 100}
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "thresholds": [
          {"colorMode": "warning", "fill": true, "line": true, "op": "gt", "value": 30},
          {"colorMode": "critical", "fill": true, "line": true, "op": "gt", "value": 40}
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "templating": {
      "list": [
        {
          "current": {
            "selected": true,
            "text": "credit-scoring-production",
            "value": "credit-scoring-production"
          },
          "hide": 0,
          "label": "Namespace",
          "name": "namespace",
          "options": [
            {
              "selected": true,
              "text": "credit-scoring-production",
              "value": "credit-scoring-production"
            },
            {
              "selected": false,
              "text": "credit-scoring-staging",
              "value": "credit-scoring-staging"
            }
          ],
          "query": "label_values(kube_namespace_labels, namespace)",
          "refresh": 1,
          "type": "query"
        },
        {
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          },
          "hide": 0,
          "label": "Pod",
          "name": "pod",
          "options": [
            {
              "selected": true,
              "text": "All",
              "value": "$__all"
            }
          ],
          "query": "label_values(kube_pod_info{namespace=\"$namespace\"}, pod)",
          "refresh": 1,
          "type": "query"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "links": [
      {
        "icon": "external link",
        "tags": [],
        "targetBlank": true,
        "title": "Runbooks",
